<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" minHeight="0.0" minWidth="0.0" prefHeight="600.0"
  prefWidth="750.0" xmlns="http://javafx.com/javafx/16" fx:controller="com.bairontapia.projects.cuidamed.mvc.ElderListView"
  fx:id="eldersListPane">
  <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
    AnchorPane.topAnchor="0.0" prefHeight="600.0" prefWidth="750.0">
    <bottom>
      <HBox BorderPane.alignment="CENTER" alignment="CENTER" prefHeight="45.0" spacing="25.0">
        <Button mnemonicParsing="false" onMouseClicked="#onViewRegistryButtonClicked"
          text="Ver adulto mayor" fx:id="viewRegisterButton">
          <font>
            <Font name="System Bold" size="12.0"/>
          </font>
        </Button>
        <Button alignment="CENTER" disable="true" mnemonicParsing="false"
          text="Ingresar adulto mayor">
          <font>
            <Font name="System Bold" size="12.0"/>
          </font>
        </Button>
      </HBox>
    </bottom>
    <center>
      <TableView BorderPane.alignment="CENTER" onMouseClicked="#onTableRegistryDoubleClicked"
        fx:id="tableView">
        <columns>
          <TableColumn editable="false" prefWidth="100.0" text="Rut" fx:id="rutColumn"/>
          <TableColumn editable="false" prefWidth="250.0" text="Nombre completo"
            fx:id="fullNameColumn"/>
          <TableColumn editable="false" prefWidth="115.0" text="Nacimiento"
            fx:id="birthDateColumn"/>
          <TableColumn editable="false" text="Edad" fx:id="ageColumn"/>
          <TableColumn editable="false" text="Sexo" fx:id="genderColumn"/>
          <TableColumn editable="false" prefWidth="115.0" text="AdmisiÃ³n" fx:id="admissionColumn"/>
        </columns>
      </TableView>
    </center>
  </BorderPane>
</AnchorPane>
