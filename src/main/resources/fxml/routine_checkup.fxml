<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="425.0"
  xmlns="http://javafx.com/javafx/11.0.1"
  fx:controller="com.bairontapia.projects.cuidamed.mvc.medical_record.RoutineCheckupView">
  <Button layoutX="253.0" layoutY="350.0" mnemonicParsing="false" onAction="#addRoutineCheckup"
    text="Aceptar"
    fx:id="add"/>
  <Button layoutX="332.0" layoutY="350.0" mnemonicParsing="false" onAction="#cancelRoutineCheckup"
    text="Cancelar"
    fx:id="cancel"/>
  <DialogPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
    AnchorPane.topAnchor="0.0"
    prefHeight="400.0" prefWidth="425.0"/>
  <HBox AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" alignment="CENTER" layoutX="-1.0"
    layoutY="2.0"
    prefHeight="75.0" spacing="25.0">
    <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@../icons/checkup.png"/>
    </ImageView>
    <Label alignment="CENTER" contentDisplay="CENTER" text="Chequeo rutinario"
      textAlignment="CENTER">
      <font>
        <Font name="System Bold" size="13.0"/>
      </font>
    </Label>
  </HBox>
  <VBox AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" layoutY="86.0" prefHeight="218.0"
    prefWidth="425.0">
    <GridPane>
      <DatePicker GridPane.columnIndex="1" editable="false" fx:id="checkupDatePicker">
        <GridPane.margin>
          <Insets left="5.0"/>
        </GridPane.margin>
      </DatePicker>
      <Label GridPane.halignment="RIGHT" text="Fecha">
        <GridPane.margin>
          <Insets right="10.0"/>
        </GridPane.margin>
      </Label>
      <Label GridPane.halignment="RIGHT" GridPane.rowIndex="1" text="Altura">
        <GridPane.margin>
          <Insets right="10.0"/>
        </GridPane.margin>
      </Label>
      <Label GridPane.halignment="RIGHT" GridPane.rowIndex="2" text="Peso">
        <GridPane.margin>
          <Insets right="10.0"/>
        </GridPane.margin>
      </Label>
      <Label GridPane.halignment="RIGHT" GridPane.rowIndex="3" text="IMC">
        <GridPane.margin>
          <Insets right="10.0"/>
        </GridPane.margin>
      </Label>
      <Label GridPane.halignment="RIGHT" GridPane.rowIndex="4" text="Ritmo cardíaco">
        <GridPane.margin>
          <Insets right="10.0"/>
        </GridPane.margin>
      </Label>
      <Label GridPane.halignment="RIGHT" GridPane.rowIndex="5" text="Presión diastólica">
        <GridPane.margin>
          <Insets right="10.0"/>
        </GridPane.margin>
      </Label>
      <Label GridPane.halignment="RIGHT" GridPane.rowIndex="6" text="Presión sistólica">
        <GridPane.margin>
          <Insets right="10.0"/>
        </GridPane.margin>
      </Label>
      <Label GridPane.halignment="RIGHT" GridPane.rowIndex="7" text="Temperatura corporal">
        <GridPane.margin>
          <Insets right="10.0"/>
        </GridPane.margin>
      </Label>
      <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" prefHeight="25.0" prefWidth="142.0"
        fx:id="height">
        <GridPane.margin>
          <Insets left="5.0" right="15.0"/>
        </GridPane.margin>
      </TextField>
      <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" fx:id="weight">
        <GridPane.margin>
          <Insets left="5.0" right="15.0"/>
        </GridPane.margin>
      </TextField>
      <TextField GridPane.columnIndex="1" GridPane.rowIndex="3" fx:id="bmi">
        <GridPane.margin>
          <Insets left="5.0" right="15.0"/>
        </GridPane.margin>
      </TextField>
      <TextField GridPane.columnIndex="1" GridPane.rowIndex="4" fx:id="heartRate">
        <GridPane.margin>
          <Insets left="5.0" right="15.0"/>
        </GridPane.margin>
      </TextField>
      <TextField GridPane.columnIndex="1" GridPane.rowIndex="5" fx:id="diastolicPressure">
        <GridPane.margin>
          <Insets left="5.0" right="15.0"/>
        </GridPane.margin>
      </TextField>
      <TextField GridPane.columnIndex="1" GridPane.rowIndex="6" fx:id="systolicPressure">
        <GridPane.margin>
          <Insets left="5.0" right="15.0"/>
        </GridPane.margin>
      </TextField>
      <TextField GridPane.columnIndex="1" GridPane.rowIndex="7" fx:id="bodyTemperature">
        <GridPane.margin>
          <Insets left="5.0" right="15.0"/>
        </GridPane.margin>
      </TextField>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="75.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      </rowConstraints>
    </GridPane>
  </VBox>
</AnchorPane>
